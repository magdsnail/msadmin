{
    "name": "open",
    "script": "app.js",
    "instances": "4",
    "exec_mode": "cluster",
    "instance_var": "INSTANCE_ID",
    "args": ["prod","9001"],
    "error_file": "./logs/open-err.log",
    "out_file": "./logs/open-out.log",
    "merge_logs": true,
    "max_memory_restart": "600M",
    "log_date_format": "YYYY-MM-DD HH:mm:ss"
  }